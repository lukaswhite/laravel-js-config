{
    "name": "kkszymanowski/laravel-js-config",
    "description": "Publish Laravel configuration to JS",
    "keywords": ["laravel", "config", "javascript"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/kkszymanowski/laravel-js-config/issues",
        "source": "https://github.com/kkszymanowski/laravel-js-config"
    },
    "authors": [
        {
            "name": "Kuba Szymanowski",
            "email": "kuba.szymanowski@inf24.pl"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/support": "~5.2|6.*",
        "illuminate/console": "~5.2|6.*",
        "illuminate/filesystem": "~5.2|6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "mockery/mockery": "^1.2",
        "orchestra/testbench": "4.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelJsConfig\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelJsConfig\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelJsConfig\\LaravelJsConfigServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
